split.form <- function(set, nrem = 2, bound = 0){
set[set$Day >= bound, paste0("Q",1:8)] <- t(apply(set[set$Day >= bound,paste0("Q",1:8)], 1, function(x){
ind <- sample(1:8, nrem)
x[ind] <- NA
as.numeric(x)
}))
return(set)
}
